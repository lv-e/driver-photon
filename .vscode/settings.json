{
    "extensions.ignoreRecommendations": true,
    "C_Cpp.default.configurationProvider": "particle.particle-vscode-core",
    "cortex-debug.openocdPath": "${command:particle.getDebuggerOpenocdPath}",
    "files.associations": {
        "*.ino": "cpp",
        "atomic": "cpp",
        "ios": "cpp",
        "__hash_table": "cpp",
        "__string": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "locale": "cpp",
        "utility": "cpp",
        "array": "cpp",
        "iterator": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "vector": "cpp",
        "filesystem": "cpp"
    },
    "C_Cpp.errorSquiggles": "Disabled",
    "files.exclude": {
        "LICENSE": true,
        ".gitignore": true,
        ".vscode": true,
        "project.properties": true
    }
}