        "__string": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "locale": "cpp",
        "utility": "cpp",
        "array": "cpp",
        "iterator": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "vector": "cpp",
        "filesystem": "cpp",
        "__split_buffer": "cpp",
        "__tree": "cpp",
        "deque": "cpp",
        "list": "cpp",
        "map": "cpp",
        "set": "cpp",
        "system_error": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "bitset": "cpp",
        "initializer_list": "cpp",
        "queue": "cpp",
        "random": "cpp",
        "regex": "cpp",
        "stack": "cpp",
        "__locale": "cpp",
        "cstddef": "cpp",
        "new": "cpp"
